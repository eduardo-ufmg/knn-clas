{
  "configurations": [
  {
    "name": "debug fit",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/bin/fit",
    "args": [
      "${workspaceFolder}/data/spirals_train.pb",
      "${workspaceFolder}/data/spirals_support.pb"
    ],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}/bin/",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
      {
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      },
      {
        "description": "Set Disassembly Flavor to Intel",
        "text": "-gdb-set disassembly-flavor intel",
        "ignoreFailures": true
      }
    ]
  },
  {
    "name": "debug pred",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/bin/pred",
    "args": [
      "${workspaceFolder}/data/spirals_test.pb",
      "${workspaceFolder}/data/spirals_support.pb",
      "${workspaceFolder}/data/spirals_predicted.pb"
    ],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}/bin/",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
      {
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      },
      {
        "description": "Set Disassembly Flavor to Intel",
        "text": "-gdb-set disassembly-flavor intel",
        "ignoreFailures": true
      }
    ]
  }
  ]
}